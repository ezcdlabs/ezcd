{
    "name": "EZCD Dev",
    "dockerComposeFile": "docker-compose.yml",
    "service": "devcontainer",
    "workspaceFolder": "/workspace",
    "forwardPorts": [
        3923
    ],
    "portsAttributes": {
        "3923": {
            "label": "Application",
            "onAutoForward": "ignore"
        }
    },
    "postCreateCommand": "./.devcontainer/setup.sh",
    "customizations": {
        "vscode": {
            "extensions": [
                "golang.go",
                "github.copilot",
                "github.copilot-chat"
            ]
        }
    }
}